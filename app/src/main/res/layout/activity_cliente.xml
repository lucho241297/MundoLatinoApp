<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#272626"
    tools:context=".Cliente">

    <ImageButton
        android:id="@+id/btnRegistrarCliente"
        android:layout_width="89dp"
        android:layout_height="89dp"
        android:layout_marginTop="196dp"
        android:background="#555252"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:onClick="Registrar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.245"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/iconocliente" />

    <ImageButton
        android:id="@+id/btnVerClientes"
        android:layout_width="89dp"
        android:layout_height="89dp"
        android:layout_marginTop="196dp"
        android:background="#555252"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:onClick="VerClientes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.779"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/iconocliente" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:adjustViewBounds="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/txtregistrar"
        android:layout_width="131dp"
        android:layout_height="22dp"
        android:layout_marginTop="16dp"
        android:text="@string/registrarClientes"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.207"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnRegistrarCliente" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="74dp"
        android:layout_height="19dp"
        android:layout_marginTop="16dp"
        android:text="@string/verClientes"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.765"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnVerClientes" />
</androidx.constraintlayout.widget.ConstraintLayout>